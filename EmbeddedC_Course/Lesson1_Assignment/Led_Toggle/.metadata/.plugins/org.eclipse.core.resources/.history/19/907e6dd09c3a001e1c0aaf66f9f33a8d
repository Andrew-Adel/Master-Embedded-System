#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif
#include "platform_types.h"

//define bases of registers
#define RCC_BASE 	0x40021000
#define GPIOA_BASE 	0x40010800
// define needed address
#define RCC_APB2ENR *(volatile uint32)(RCC_BASE+ 0x18)
#define GPIA_PA_CHR *(volatile uint32)(GPIOA_BASE+ 0x04)
#define GPIA_PA_ODR *(volatile uint32)(GPIOA_BASE+ 0x0c)

#define RCC_IOPAEN_BIT = 1<<2

int main(void)
{
	RCC_APB2ENR |= RCC_IOPAEN_BIT;

	for(;;);
}

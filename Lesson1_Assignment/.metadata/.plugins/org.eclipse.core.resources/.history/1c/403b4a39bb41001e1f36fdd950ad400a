#include "platform_types.h"
#ifndef LIFO_H_
#define LIFO_H_

typedef struct {
	uint32 length;
	uint32 count;
	uint32* base;
	uint32* head;
}LIFO_Buf_t;

typedef enum{
	LIFO_no_error,
	LIFO_full,
	LIFO_empty,
	LIFO_Null
}LIFO_status;

#endif

 */

#include "fifo.h"
int main(){
	FIFO_Buf_t fifo_UART;
	element_type i, temp;
	if (FIFO_Init(&fifo_UART,uart_buffer, width1) == FIFO_no_error) {
		print("FIFO init ----- Done\n");
	} else {
		print("FIFO init ----- Faild");
		exit(0);
	}
	for (i = 0; i < width1; ++i) {
		print("FIFO enqueue (%x):\n",i);
		if (FIFO_enqueue(&fifo_UART,i) == FIFO_no_error) {
			print("\tFIFO enqueue ----- Done\n");
		} else {
			print("FIFO init ----- Faild");
			exit(0);
		}
	}

	return 0;
}

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <conio.h>

typedef struct {
	int ID;
	char name[40];
	float height;
} SData;

typedef struct {
	SData Student_Data;
	SStudent* PNextStudent;
} SStudent;

SStudent* gpFirstStudent = NULL;
